{
  "name": "order-api",
  "version": "1.0.0",
  "description": "This is the example from the Book Hands on RESTful Web Services with TypeScript 3",
  "main": "./dist/server.js",
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rimraf dist && rimraf reports",
    "lint": "tslint ./src/**/*.ts ./test/**/*.spec.ts",
    "lint:fix": "tslint --fix ./src/**/*.ts ./test/**/*.spec.ts -t verbose",
    "pretest": "cross-env NODE_ENV=test npm run build && npm run lint",
    "test": "cross-env NODE_ENV=test mocha --reporter spec --compilers ts:ts-node/register test/**/*.spec.ts --exit",
    "test:mutation": "stryker run",
    "stryker:init": "stryker init",
    "mondev": "nodemon dev",
    "dev": "nodemon",
    "prod": "PORT=3000 npm run build && npm run start",
    "tsc": "tsc",
    "test-DB-connection": "ts-node ./src/database.ts"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "keywords": [
    "Akatosh",
    "",
    "",
    ""
  ],
  "author": "akatosh",
  "lisense": "MIT",
  "devDependencies": {
    "@stryker-mutator/core": "^5.4.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.7",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.16.0",
    "@types/mocha": "^5.2.5",
    "@types/mysql": "^2.15.19",
    "@types/node": "^10.12.12",
    "@types/pg": "^8.6.1",
    "chai": "^4.2.0",
    "cross-env": "^5.2.0",
    "mocha": "^5.2.0",
    "nodemon": "^2.0.14",
    "rimraf": "^2.6.2",
    "stryker": "^0.33.1",
    "stryker-api": "^0.22.0",
    "stryker-html-reporter": "^0.16.9",
    "stryker-mocha-framework": "^0.13.2",
    "stryker-mocha-runner": "^0.15.2",
    "stryker-typescript": "^0.16.1",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^3.2.1"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "chai-http": "^4.2.1",
    "class-validator": "^0.13.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.16.4",
    "express-winston": "^4.2.0",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "pg": "^8.7.1",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.6.5",
    "typeorm": "^0.2.38",
    "uuid": "^8.3.2",
    "winston": "^3.3.3"
  }
}
