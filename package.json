{
  "name": "akatosh-api",
  "version": "0.1.0",
  "description": "Akatosh API",
  "main": "./dist/server.js",
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rimraf dist && rimraf reports",
    "lint": "tslint ./src/**/*.ts ./test/**/*.spec.ts",
    "lint:fix": "tslint --fix ./src/**/*.ts ./test/**/*.spec.ts -t verbose",
    "pretest": "cross-env NODE_ENV=test npm run build && npm run lint",
    "test": "cross-env NODE_ENV=test mocha --reporter spec --compilers ts:ts-node/register test/**/*.spec.ts --exit",
    "test:mutation": "stryker run",
    "stryker:init": "stryker init",
    "mondev": "nodemon dev",
    "dev": "cross-env PORT=3000 NODE_ENV=dev ts-node ./src/server.ts",
    "prod": "PORT=3000 npm run build && npm run start",
    "test-DB-connection": "ts-node ./src/database.ts",
    "prestart": "node dist/config.js",
    "start": "NODE_ENV=prod node dist/server.js",
    "tsc": "tsc"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "keywords": [
    "Akatosh",
    "",
    "",
    ""
  ],
  "author": "akatosh",
  "lisense": "MIT",
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.7",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.16.0",
    "@types/jsonwebtoken": "^8.5.5",
    "@types/mocha": "^9.1.0",
    "@types/mysql": "^2.15.19",
    "@types/node": "^17.0.18",
    "chai": "^4.2.0",
    "cross-env": "^7.0.3",
    "mocha": "^9.2.0",
    "nodemon": "^2.0.14",
    "rimraf": "^3.0.2",
    "ts-node": "^10.5.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@types/pg": "^8.6.4",
    "body-parser": "^1.18.3",
    "chai-http": "^4.2.1",
    "class-validator": "^0.13.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.16.4",
    "express-oauth2-jwt-bearer": "^1.1.0",
    "express-session": "^1.17.2",
    "express-winston": "^4.2.0",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.2.38",
    "uuid": "^8.3.2",
    "winston": "^3.3.3",
    "winston-aws-cloudwatch": "^3.0.0"
  }
}
